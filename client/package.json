{
  "name": "saudicord-client",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "set DANGEROUSLY_DISABLE_HOST_CHECK=true && react-scripts start",
